<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="new1" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorAccent">@color/accent_material_light_2</item>
    </style>

    <style name="new2" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorAccent">@android:color/holo_blue_dark</item>
    </style>

    <style name="dialog" parent="Theme.AppCompat.NoActionBar" >
        <item name="android:background">@color/accent_material_light_2</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

</resources>
